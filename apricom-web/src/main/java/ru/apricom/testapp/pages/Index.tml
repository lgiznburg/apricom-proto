<html t:type="layout" t:pageTitle="APriCom Главная"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter">

<div class="content">
    <div class="row">
        <div class="col-sm-10 col-sm-offset-2">
        <h4>Начало</h4>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-10 col-sm-offset-2">
            <t:pagelink page="requests/wizard" class="btn btn-success">Создать заявку</t:pagelink>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-10 col-sm-offset-2">
            <t:pagelink page="requests/list" class="btn btn-success">Список заявок</t:pagelink>
        </div>
    </div>
</div>

</html>